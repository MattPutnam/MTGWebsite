<table class="{{eval('$header'.replace(' ', '_'))}}">
    <thead>
        <tr>
            <th colspan="2">{{$header}}</th>
        </tr>
    </thead>
    <tbody>{{foreach:var=row, source=$content}}
        <tr>
            <td class="role">{{$row/role}}</td>
            <td class="name">{{$row/name}}</td>
        </tr>{{end}}
    </tbody>
</table>